public class E1NomWhi
{
  public static void main(String[] args) 
  {
      String[] nombres = {"Ana", "Luis", "María", "Carlos", "Sofía", "Jorge", "Lucía", "Pedro", "Elena", "Miguel" };
        int i = 0; 
      while (i < 10) {
            System.out.println((i + 1) + ". " + nombres[i]);
            i++;
                
        }
        
    }
}
